{{>layouts/header}}
<form action="/articles/create" method="post">
    <div class="mb-3">
        <label for="exampleInputSubject" class="form-label">제목</label>
        <input type="text" class="form-control" id="exampleInputSubject" aria-describedby="subjectHelp" name="title">
    </div>
    <div class="mb-3">
        <label for="exampleInputContent" class="form-label">내용</label>
        <textarea class="form-control" id="exampleInputContent" rows="3" name="content"></textarea>
    </div>
    <button type="submit" class="btn btn-primary">Submit</button>
</form>
{{>layouts/footer}}